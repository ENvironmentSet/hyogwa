{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "noEmit": true,
    "exactOptionalPropertyTypes": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  },
  "exclude": [
    "node_modules"
  ]
}